<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="lp.JavaFxClient.Controllers.ParticipanteInscricoesController"
      spacing="10"
      style="-fx-padding:15;">

    <Label text="Minhas Inscrições"
           style="-fx-font-size:16; -fx-font-weight:bold;"/>

    <TableView fx:id="inscricoesTable">
        <columns>
            <TableColumn fx:id="colEvento"
                         text="Evento"
                         prefWidth="220"/>
            <TableColumn fx:id="colEstado"
                         text="Estado"
                         prefWidth="120"/>
            <TableColumn fx:id="colData"
                         text="Data"
                         prefWidth="160"/>
        </columns>
    </TableView>

    <HBox spacing="10">
    <Pane HBox.hgrow="ALWAYS"/>
    <Button text="Avaliar Evento"
            onAction="#onAvaliar"/>
    <Button text="Cancelar Inscrição"
            onAction="#onCancelar"/>
</HBox>


</VBox>
