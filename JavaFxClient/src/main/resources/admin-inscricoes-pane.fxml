<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="lp.JavaFxClient.Controllers.AdminInscricoesController"
      spacing="12">

    <padding>
        <Insets top="15" right="15" bottom="15" left="15"/>
    </padding>

    <Label text="Inscrições"
           style="-fx-font-size: 20; -fx-font-weight: bold;" />

<TableView fx:id="inscricoesTable">
  <columns>
    <TableColumn fx:id="colId" text="ID"/>
    <TableColumn fx:id="colEvento" text="Evento"/>
    <TableColumn fx:id="colParticipante" text="Participante"/>
    <TableColumn fx:id="colEstado" text="Estado"/>
  </columns>
</TableView>


</VBox>
